{
    "name": "Telegram proxy",
    "version": "release-0.7.1",
    "slug": "telegram-proxy",
    "description": "The Telegram Messenger MTProto proxy is a zero-configuration container that automatically sets up a proxy server that speaks Telegram's native MTProto.",
    "url": "https://github.com/ad/ha-telegram-proxy",
    "startup": "system",
    "arch": [
        "aarch64",
        "amd64",
        "armhf",
        "armv7"
    ],
    "hassio_api": false,
    "init": false,
    "options": {
        "MTP_SECRET": "",
        "MTP_TAG": "",
        "MTP_PORT": "4443",
        "MTP_DD_ONLY": "f",
        "MTP_TLS_ONLY": "f"
    },
    "schema": {
        "MTP_SECRET": "str",
        "MTP_TAG": "str",
        "MTP_PORT": "int(0,65535)", 
        "MTP_DD_ONLY": "bool",
        "MTP_TLS_ONLY": "bool"
    },
    "image": "docker.io/seriyps/mtproto-proxy"
  }
  
